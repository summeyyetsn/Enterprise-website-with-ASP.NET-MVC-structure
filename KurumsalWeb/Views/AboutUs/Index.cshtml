@model IEnumerable<KurumsalWeb.Models.Model.AboutUs>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Explanation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.Raw(item.Explanation)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AboutUsId })
        </td>
    </tr>
}

</table>
